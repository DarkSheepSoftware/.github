{
  "extends": [
    ":dependencyDashboard",
    ":dependencyDashboardApproval",
    ":disableRateLimiting",
    ":enableVulnerabilityAlerts",
    ":ignoreModulesAndTests",
    ":ignoreUnstable",
    ":separateMultipleMajorReleases",
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all"
  ],
  "configMigration": true,
  "rangeStrategy": "update-lockfile",
  "lockFileMaintenance": { "enabled": true },
  "npmrc": "@pickles-and-beans:registry=https://npm.pkg.github.com/",
  "prBodyTemplate": "{{{table}}}{{{warnings}}}{{{notes}}}{{{changelogs}}}{{{controls}}}{{{footer}}}",
  "prBodyColumns": [ "Package", "Update", "Type", "Change" ],
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [ "*" ],
      "semanticCommits": "enabled",
      "semanticCommitType": "deps"
    },
    {
      "matchDepTypes": [ "dependencies" ],
      "semanticCommitScope": "pkg"
    },
    {
      "matchDepTypes": [ "devDependencies" ],
      "semanticCommitScope": "dev"
    },
    {
      "matchDepTypes": [ "peerDependencies" ],
      "semanticCommitScope": "peer"
    }
  ]
}
